{
    "presets": [
        "@babel/preset-env",
        [
            "@babel/preset-react",
            {
                "runtime": "automatic"
            }
        ]
    ],
    "plugins": ["@babel/transform-runtime",
        [
            "babel-plugin-styled-components",
            {
                "ssr": false,
                "displayName": true,
                "fileName": false,
                "pure": true,
                "namespace": "react-onboarding"
            }
        ]
    ]
}